{
  "name": "trident",
  "displayName": "Trident",
  "description": "Trident â€” universal language for provable computation",
  "version": "0.1.0",
  "publisher": "mastercyb",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:trident"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "trident",
        "aliases": [
          "Trident",
          "trident"
        ],
        "extensions": [
          ".tri"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "tasm",
        "aliases": [
          "TASM"
        ],
        "extensions": [
          ".tasm"
        ]
      }
    ],
    "grammars": [
      {
        "language": "trident",
        "scopeName": "source.trident",
        "path": "./syntaxes/trident.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -watch -p ."
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.0"
  }
}
